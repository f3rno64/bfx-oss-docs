[
  0,
  "fon",
  null,
  {
    type: TYPE,
    symbol: SYMBOL,
    amount: AMOUNT,
    rate: RATE,
    period: PERIOD,
    flags: FLAGS
  }
]

// Examples
[
  0, 
  'fon', 
  null, 
  { 
    type: "LIMIT", 
    symbol: "fUSD", 
    amount: '50', 
    rate: '0.0002', 
    period: 7, 
    flags: 0 
