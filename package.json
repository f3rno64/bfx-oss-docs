{
  "name": "bfx-oss-docs",
  "version": "1.0.0",
  "description": "Official Bitfinex Documentation",
  "main": "index.js",
  "engines": {
    "node": ">=14.2.0"
  },
  "homepage": "https://github.com/bitfinexcom/bfx-oss-docs#readme",
  "license": "Apache-2.0",
  "contributors": [
    "Cris Mihalache <cris.m@bitfinex.com> (https://www.bitfinex.com)",
    "Paolo Ardoino <paolo@bitfinex.com> (https://www.bitfinex.com)",
    "Jacob Plaster <jacob.plaster@bitfinex.com> (https://www.bitfinex.com)"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bitfinexcom/bfx-oss-docs.git"
  },
  "bugs": {
    "url": "https://github.com/bitfinexcom/bfx-oss-docs/issues"
  },
  "keywords": [
    "bitfinex",
    "jsdoc",
    "library",
    "trading",
    "indicators",
    "automation",
    "framework"
  ],
  "scripts": {
    "lint": "./node_modules/.bin/eslint --ext .js ./lib/index.js ./lib/js ./test/",
    "unit": "NODE_ENV=test ./node_modules/.bin/nyc ./node_modules/.bin/mocha --recursive",
    "test": "npm run lint && npm run unit",
    "clean": "npm run clean-jsdoc && npm run clean-site",
    "clean-jsdoc": "rm -rf ./dist/jsdoc/* && touch ./dist/jsdoc/.keep",
    "clean-site": "rm -rf ./dist/site/* && touch ./dist/site/.keep",
    "init-repo": "npm run init-submodules && npm run init-hf-chart && npm run init-zepto",
    "init-submodules": "git submodule init && git submodule update",
    "init-hf-chart": "cd node_modules/bfx-hf-chart && npm run install && npm run cra-build",
    "init-zepto": "cd _assets/npm/zepto && npm i && npm run dist",
    "serve-site": "./node_modules/.bin/http-server ./dist/site",
    "serve-jsdoc": "./node_modules/.bin/http-server ./dist/jsdoc",
    "build-site": "npm run clean-site && npm run build-site-styles && npm run build-site-assets && npm run build-site-scripts && npm run build-site-content",
    "build-site-styles": "node scripts/render_styles.js",
    "build-site-assets": "node scripts/render_assets.js",
    "build-site-scripts": "node scripts/render_scripts.js",
    "build-site-content": "node scripts/render_content.js",
    "build-libs": "npm run build-libs-js && npm run build-libs-rb && npm run build-libs-py && npm run build-libs-go",
    "build-libs-js": "node scripts/render_libraries_js.js",
    "build-libs-rb": "node scripts/render_libraries_rb.js",
    "build-libs-py": "node scripts/render_libraries_py.js",
    "build-libs-go": "node scripts/render_libraries_go.js",
    "build-jsdoc-assets": "cp ./jsdoc_assets/* ./dist/jsdoc/ -r",
    "build-jsdoc-config": "node scripts/render_jsdoc_config.js",
    "build-jsdoc-config-json": "node scripts/render_jsdoc_config.js json",
    "build-jsdoc-dist": "./node_modules/.bin/jsdoc --pedantic --configure .jsdoc-merged.json --verbose",
    "build-jsdoc-html": "npm run build-jsdoc-config && npm run build-jsdoc-dist && npm run build-jsdoc-assets",
    "build-jsdoc-json": "npm run build-jsdoc-config-json && npm run build-jsdoc-dist",
    "build": "npm run build-libs && npm run build-site",
    "dist": "npm run clean && npm run init-repo && npm run build"
  },
  "dependencies": {
    "@babel/core": "^7.10.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.0",
    "@babel/preset-env": "^7.10.0",
    "@centerforopenscience/markdown-it-imsize": "^2.0.1",
    "@gerhobbelt/markdown-it-hashtag": "^0.4.0-1",
    "@tomleesm/markdown-it-wikilinks": "^1.1.1",
    "bfx-api-node-util": "^1.0.9",
    "bfx-hf-chart": "^1.0.5",
    "bluebird": "^3.7.2",
    "browserify": "^16.5.1",
    "debug": "^4.3.1",
    "fs-extra": "^9.0.0",
    "highlight.js": "^10.0.3",
    "highlightjs-line-numbers.js": "^2.8.0",
    "html-minifier": "^4.0.0",
    "js-yaml": "^3.14.0",
    "jsdoc-json": "^2.0.2",
    "lodash": "^4.17.21",
    "markdown-it": "^10.0.0",
    "markdown-it-anchor": "^5.3.0",
    "markdown-it-collapsible": "^1.0.0",
    "markdown-it-container": "^2.0.0",
    "markdown-it-front-matter": "^0.2.1",
    "markdown-it-grouped-code-fence": "^1.2.0",
    "markdown-it-highlightjs": "^3.1.0",
    "markdown-it-imsize": "^2.0.1",
    "markdown-it-smartarrows": "^1.0.1",
    "markdown-it-task-lists": "^2.1.1",
    "markdown-it-toc-done-right": "^4.1.0",
    "normalize.css": "^8.0.1",
    "nunjucks": "^3.2.4",
    "nunjucks-extension-code-highlight": "^0.0.1",
    "nunjucks-markdown": "^2.0.1",
    "p-iteration": "^1.1.8",
    "perfect-scrollbar": "^1.5.0",
    "sass": "^1.26.5",
    "signale": "^1.4.0",
    "slugify": "^1.4.0",
    "string": "^3.3.3",
    "stylus": "^0.54.7",
    "svg-icon": "^0.8.2",
    "uslug": "^1.0.4",
    "zepto": "^1.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "concurrently": "^5.2.0",
    "docdash": "^1.2.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsdoc": "^22.0.0",
    "eslint-plugin-lodash": "^6.0.0",
    "eslint-plugin-mocha": "^6.3.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "http-server": "^0.12.1",
    "husky": "^4.2.3",
    "jsdoc": "^3.6.4",
    "jsdoc-plugin-npm": "^1.0.3",
    "jsdoc-plugin-url": "^1.0.1",
    "jsdoc-to-markdown": "^5.0.3",
    "mocha": "^7.1.0",
    "nyc": "^15.0.0",
    "standard": "^14.1.0"
  }
}
