{
  "name": "bfx-oss-docs",
  "version": "1.0.0",
  "description": "Official Bitfinex Documentation",
  "main": "index.js",
  "engines": {
    "node": ">=14.2.0"
  },
  "homepage": "https://github.com/bitfinexcom/bfx-oss-docs#readme",
  "license": "Apache-2.0",
  "contributors": [
    "Cris Mihalache <cris.m@bitfinex.com> (https://www.bitfinex.com)",
    "Paolo Ardoino <paolo@bitfinex.com> (https://www.bitfinex.com)",
    "Jacob Plaster <jacob.plaster@bitfinex.com> (https://www.bitfinex.com)"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bitfinexcom/bfx-oss-docs.git"
  },
  "bugs": {
    "url": "https://github.com/bitfinexcom/bfx-oss-docs/issues"
  },
  "keywords": [
    "bitfinex",
    "jsdoc",
    "library",
    "trading",
    "indicators",
    "automation",
    "framework"
  ],
  "scripts": {
    "lint": "./node_modules/.bin/eslint --ext .js ./lib/index.js ./lib/js ./test/",
    "unit": "NODE_ENV=test ./node_modules/.bin/nyc ./node_modules/.bin/mocha --recursive",
    "test": "pnpm lint && pnpm unit",
    "clean": "pnpm clean-jsdoc && pnpm clean-site",
    "clean-jsdoc": "rm -rf ./dist/jsdoc/* && touch ./dist/jsdoc/.keep",
    "clean-site": "rm -rf ./dist/site/* && touch ./dist/site/.keep",
    "init-repo": "pnpm init-submodules && pnpm init-hf-chart && pnpm init-zepto",
    "init-submodules": "git submodule init && git submodule update",
    "init-hf-chart": "cd node_modules/bfx-hf-chart && pnpm install && pnpm cra-build",
    "init-zepto": "cd _assets/npm/zepto && pnpm install && pnpm dist",
    "serve-site": "./node_modules/.bin/http-server ./dist/site",
    "serve-jsdoc": "./node_modules/.bin/http-server ./dist/jsdoc",
    "build-site": "pnpm clean-site && pnpm build-site-styles && pnpm build-site-assets && pnpm build-site-scripts && pnpm build-site-content",
    "build-site-styles": "node scripts/render_styles.js",
    "build-site-assets": "node scripts/render_assets.js",
    "build-site-scripts": "node scripts/render_scripts.js",
    "build-site-content": "node scripts/render_content.js",
    "build-libs": "pnpm build-libs-js && pnpm build-libs-rb && pnpm build-libs-py && pnpm build-libs-go",
    "build-libs-js": "node scripts/render_libraries_js.js",
    "build-libs-rb": "node scripts/render_libraries_rb.js",
    "build-libs-py": "node scripts/render_libraries_py.js",
    "build-libs-go": "node scripts/render_libraries_go.js",
    "build-jsdoc-assets": "cp ./jsdoc_assets/* ./dist/jsdoc/ -r",
    "build-jsdoc-config": "node scripts/render_jsdoc_config.js",
    "build-jsdoc-config-json": "node scripts/render_jsdoc_config.js json",
    "build-jsdoc-dist": "./node_modules/.bin/jsdoc --pedantic --configure .jsdoc-merged.json --verbose",
    "build-jsdoc-html": "pnpm build-jsdoc-config && pnpm build-jsdoc-dist && pnpm build-jsdoc-assets",
    "build-jsdoc-json": "pnpm build-jsdoc-config-json && pnpm build-jsdoc-dist",
    "build": "pnpm build-libs && pnpm build-site",
    "dist": "pnpm clean && pnpm init-repo && pnpm build"
  },
  "dependencies": {
    "@babel/core": "^7.23.9",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.0",
    "@babel/preset-env": "^7.23.9",
    "@centerforopenscience/markdown-it-imsize": "^2.0.1",
    "@gerhobbelt/markdown-it-hashtag": "^0.4.0-1",
    "@tomleesm/markdown-it-wikilinks": "^1.1.1",
    "bfx-api-node-util": "^1.0.11",
    "bfx-hf-chart": "^1.0.6",
    "bluebird": "^3.7.2",
    "browserify": "^17.0.0",
    "debug": "^4.3.4",
    "fs-extra": "^11.2.0",
    "highlight.js": "^11.9.0",
    "highlightjs-line-numbers.js": "^2.8.0",
    "html-minifier": "^4.0.0",
    "js-yaml": "^4.1.0",
    "jsdoc-json": "^2.0.2",
    "lodash": "^4.17.21",
    "markdown-it": "^14.0.0",
    "markdown-it-anchor": "^8.6.7",
    "markdown-it-collapsible": "^2.0.0",
    "markdown-it-container": "^4.0.0",
    "markdown-it-front-matter": "^0.2.3",
    "markdown-it-grouped-code-fence": "^1.2.0",
    "markdown-it-highlightjs": "^4.0.1",
    "markdown-it-imsize": "^2.0.1",
    "markdown-it-smartarrows": "^1.0.1",
    "markdown-it-task-lists": "^2.1.1",
    "markdown-it-toc-done-right": "^4.2.0",
    "normalize.css": "^8.0.1",
    "nunjucks": "^3.2.4",
    "nunjucks-extension-code-highlight": "^0.0.1",
    "nunjucks-markdown": "^2.0.1",
    "p-iteration": "^1.1.8",
    "perfect-scrollbar": "^1.5.5",
    "sass": "^1.70.0",
    "signale": "^1.4.0",
    "slugify": "^1.6.6",
    "string": "^3.3.3",
    "stylus": "^0.62.0",
    "svg-icon": "^0.8.2",
    "uslug": "^1.0.4",
    "zepto": "^1.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "chai": "^5.1.0",
    "concurrently": "^8.2.2",
    "docdash": "^2.0.2",
    "eslint": "^8.56.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.1.0",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-mocha": "^10.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^4.1.0",
    "http-server": "^14.1.1",
    "husky": "^9.0.11",
    "jsdoc": "^4.0.2",
    "jsdoc-plugin-npm": "^1.0.3",
    "jsdoc-plugin-url": "^1.0.1",
    "jsdoc-to-markdown": "^8.0.1",
    "mocha": "^10.3.0",
    "nyc": "^15.1.0",
    "standard": "^17.1.0"
  },
  "pnpm": {
    "overrides": {
      "braces@<2.3.1": ">=2.3.1",
      "js-yaml@<3.13.0": ">=3.13.0",
      "ecstatic@<4.1.3": ">=4.1.3",
      "shelljs@<0.8.5": ">=0.8.5",
      "node-forge@<1.0.0": ">=1.0.0",
      "node-forge@<1.3.0": ">=1.3.0",
      "flat@<5.0.1": ">=5.0.1",
      "ejs@<3.1.7": ">=3.1.7",
      "immer@<9.0.6": ">=9.0.6",
      "xmldom@<0.7.0": ">=0.7.0",
      "color-string@<1.5.5": ">=1.5.5",
      "xmldom@<0.5.0": ">=0.5.0",
      "xml2js@<0.5.0": ">=0.5.0",
      "markdown-it@<12.3.2": ">=12.3.2",
      "browserslist@>=4.0.0 <4.16.5": ">=4.16.5",
      "loader-utils@>=2.0.0 <2.0.4": ">=2.0.4",
      "loader-utils@>=2.0.0 <2.0.3": ">=2.0.3",
      "debug@>=4.0.0 <4.3.1": ">=4.3.1",
      "debug@>=3.2.0 <3.2.7": ">=3.2.7",
      "postcss@<8.4.31": ">=8.4.31",
      "ansi-html@<0.0.8": ">=0.0.8",
      "glob-parent@<5.1.2": ">=5.1.2",
      "marked@<4.0.10": ">=4.0.10",
      "js-yaml@<3.13.1": ">=3.13.1",
      "tough-cookie@<4.1.3": ">=4.1.3",
      "nth-check@<2.0.1": ">=2.0.1",
      "shell-quote@<=1.7.2": ">=1.7.3",
      "semver@>=7.0.0 <7.5.2": ">=7.5.2",
      "minimatch@<3.0.5": ">=3.0.5"
    }
  }
}
